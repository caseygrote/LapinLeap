<Window xmlns:LapinLeap="clr-namespace:LapinLeap"  x:Class="LapinLeap.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="600" Width="1018">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid x:Name="bgGrid" Background="#FF1A2951" Margin="0,-1,0,-11"/>
        <LapinLeap:GameGrid x:Name="level" Margin="242,21,22,10"  />
        <LapinLeap:Inventory x:Name="inventory" HorizontalAlignment="Left" Margin="21,21,0,10" Height="Auto" Width="211"/>
        <Label x:Name="DoomsdayClock"  Foreground="White" Margin="0,-13,0,0" FontFamily="Franklin Gothic Heavy" FontSize="50" Height="110" VerticalAlignment="Top" HorizontalAlignment="Right" Width="155" >
            00:00
        </Label>
        <Canvas x:Name="MapCanvas" Background="#FFDE0404"  Margin="242,21,22,10" Visibility="Collapsed" PreviewMouseDown="MapCanvas_PreviewMouseDown" >
            <Label>
                a map ?? incredible
            </Label>
        </Canvas>
    </Grid>
</Window>
